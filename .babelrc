{
    "plugins": [
        "babel-plugin-transform-class-properties",
        "babel-plugin-transform-object-rest-spread"
    ],
    "presets": [
        "babel-preset-react",
        ["babel-preset-env", {
            "targets": {
                "browsers": ["last 2 versions", "ie 10", "safari >= 10"],
                "uglify": true,
                "useBuiltIns": true,
                "modules": false
            }
        }]
    ]
}
